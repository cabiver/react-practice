                <div class="plus-camara-div">
                    <img  src="images/mas.png" alt="">
                </div>
                <div class="camara-background feedCamara">
                    <img class="imagen-de-camara" id="changeBackground" src="images/camara.png" alt="">
                </div>
        </div>


    </div>


    <div id="responsive_cuentas.js-para_hacer_margin_top" class="background background-height">

        <div id="div-image-background" class="div-image-background">

            <%- extencion %>


        </div>


    </div>

    <div id="divBorderCamaraIcono" class="divBorderCamaraIcono">
