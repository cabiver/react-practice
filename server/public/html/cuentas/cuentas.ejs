<header id="Header">

    <body>
        <div id="gridChangeBackground" style="display:none">
            <div id="closeChangeBackground"> <img src="images/close-x.png" alt=""> </div>

            <div class="divChangeBackground">
                <form id="fromChangeBackground" action="">
                    <input id="backgroundFile" type="file" style="display: none" name="image">
                    <label for="">cambia de fondo aqui</label>
                    <label class="buttonLable" for="backgroundFile">subir</label>
                    <img id="renderBackground" src="" alt="">
                    <button>enviar fondo</button>
                    <label id="respuestaBackground"></label>
                </form>
            </div>

        </div>

        <div id="responsive_cuentas.js-calcular_y_asignar_width_a_la_cabecera_y_detectar_click" class="cabecera">
            <div id="responsive_cuentas.js cuentas.js-calcular_distancia_y_esperar_click">
                <div id="iconoDeLaPagina" class="separadorDeCabecera">
                    <img class="icono js-detectar_cuando_carge" src="images/camille-300x300.png" alt="">
                </div>
            </div>
            <div id="responsive_cuentas.js-referencia_para_calcular_la_pocision_de_la_lupa-barra_de_busqueda-div_barra_busqueda">
                <form id="responsive_cuentas.js-variable_para_el_posicionamiento_de_la_lupa-form_busqueda" class="barraDeBusqueda" >
                    <input id="responsive_cuentas.js-variable_de_posicionamiento" autocomplete="off" name="usuarios" class="inputs-text-sin-mamadas div-dentro-de-barra-de-busqueda" action="" type="text">
                    <button id="responsive_cuentas.js-posicionar_la_lupa" class="fit-height">
                        <img id="lupa" class="icono-lupa" src="images/lupa.png" >
                    </button>
                  
    
                </form>
                <div style="display:flex; justify-content:center">
                    <div id="responsive_cuentas.js cuentas.js-posicionamiento_y_actualizacion_de_peticiones_de_recomendados" class="divRecomiendaciones"></div>
                </div>
                
            </div>

                <div id="responsive_cuentas.js-div_de_tu_perfil_para_ajustar_barra_de_busqueda" class="div-perfil-tuyo">
                    <div class="separadorDeCabecera div-icono-de-usuario" id="cuentas.js-agregar_eventos_que_desplegue_opciones_de_usuarios">
                        <div class="responsive_cuentas.js-arrays_de_iconos_para_volverlos_cuadrados iconPerfil">
                            <img class="iconoPrincipal"  src="<%= icon %>">
                        </div>
                        
                        <div class="nombre">
                            <%= name.length>7?`${name.substr(0,7)}...`:name %>
                        </div>
                    </div>
        
                </div>
            
            
        </div>

        <div id="barra-de-navegacion" class="Flex-navegador-user" style="display:none">
            <button id="ir-a-mi-sitio" class="p-de-navegador">Ir a mi sitio web</button>
            <button id="cerrarSesion" class="p-de-navegador">cerrar Sesion</button>
        </div>

</header>

